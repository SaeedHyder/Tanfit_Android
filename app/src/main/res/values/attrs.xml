<?xml version="1.0" encoding="utf-8"?>
<resources>

  <declare-styleable name="AutoCompleteLocation">
    <attr name="background_layout"
        format="integer"/>
    <attr name="hint_text"
        format="string"/>
    <attr name="hint_text_color"
        format="color"/>
    <attr name="text_color"
        format="color"/>
  </declare-styleable>
  <declare-styleable name="SwipeDeck">
    <attr name="max_visible" format="integer"/>
    <attr name="rotation_degrees" format="float"/>
    <attr name="card_spacing" format="dimension"/>
    <attr name="render_above" format="boolean"/>
    <attr name="render_below" format="boolean"/>
    <attr name="opacity_end" format="float"/>
    <attr name="card_gravity" format="enum">
      <enum name="top" value="0"/>
      <enum name="center" value="1"/>
      <enum name="bottom" value="2"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="slideunlocker">
    <attr name="slider_text" format="string"/>
    <attr name="slider_thumb" format="integer"/>
    <attr name="slider_track" format="integer"/>
  </declare-styleable>
</resources>